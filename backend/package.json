{
  "name": "ivs-live-streaming-backend",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json && npm run copy-templates",
    "copy-templates": "cp -r src/templates dist/",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "seed": "tsx src/scripts/seed.ts",
    "seed:invoices": "tsx src/scripts/seed-invoices.ts",
    "regenerate:invoices": "tsx src/scripts/regenerate-invoices.ts",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@aws-sdk/client-ivs": "^3.956.0",
    "@aws-sdk/client-s3": "^3.957.0",
    "@aws-sdk/cloudfront-signer": "^3.957.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.13.1",
    "puppeteer": "^23.11.1",
    "razorpay": "^2.9.6",
    "resend": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.16.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.0"
  }
}
